{
  "name": "prisma",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/prisma/src",
  "projectType": "library",
  "release": {
    "version": {
      "manifestRootsToUpdate": ["dist/{projectRoot}"],
      "currentVersionResolver": "git-tag",
      "fallbackCurrentVersionResolver": "disk"
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/prisma",
        "main": "packages/prisma/src/index.ts",
        "tsConfig": "packages/prisma/tsconfig.lib.json",
        "assets": ["packages/prisma/*.md"]
      }
    },
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma generate --schema=packages/prisma/src/lib/schema.prisma"
      }
    },
    "migrate-dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma migrate dev --schema=packages/prisma/src/lib/schema.prisma"
      }
    },
    "migrate-deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma migrate deploy --schema=packages/prisma/src/lib/schema.prisma"
      }
    },
    "studio": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma studio --schema=packages/prisma/src/lib/schema.prisma"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma format --schema=packages/prisma/src/lib/schema.prisma"
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    }
  }
}
